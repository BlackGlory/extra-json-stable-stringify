{
  "name": "ultra-json-stable-stringify",
  "version": "0.1.0",
  "description": "",
  "keywords": [],
  "files": [
    "lib",
    "dist"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "sideEffects": false,
  "repository": "git@github.com:BlackGlory/ultra-json-stable-stringify.git",
  "author": "BlackGlory <woshenmedoubuzhidao@blackglory.me>",
  "license": "MIT",
  "scripts": {
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --quiet src __tests__",
    "test": "jest --runInBand --config jest.config.js",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "test:coverage": "jest --coverage --config jest.config.js",
    "prepublishOnly": "run-s clean build bundle",
    "clean": "run-p clean:*",
    "clean:build": "rimraf lib",
    "clean:bundle": "rimraf dist",
    "clean:wasm": "rimraf wasm/lib",
    "build": "run-p build:*",
    "build:compile": "tsc --project tsconfig.build.json --module commonjs",
    "build:patch": "tscpaths -p tsconfig.build.json -s ./src -o ./lib",
    "bundle": "rollup --config rollup.config.js",
    "bench": "./benches/benchmark.sh",
    "unused:build:wasm": "run-p unused:build:wasm:*",
    "unused:build:wasm:nodejs": "wasm-pack build --release --target nodejs --out-dir lib/nodejs wasm",
    "unused:build:wasm:web": "wasm-pack build --release --target bundler --out-dir lib/web wasm",
    "release": "standard-version"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s lint build test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@blackglory/jest-matchers": "^0.3.1",
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-replace": "^4.0.0",
    "@rollup/plugin-typescript": "^8.4.0",
    "@types/jest": "^27.4.1",
    "@types/node": "^18.7.13",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.35.1",
    "eslint": "^8.22.0",
    "extra-benchmark": "^0.1.0",
    "extra-filesystem": "^0.4.5",
    "fast-json-stable-stringify": "^2.1.0",
    "fast-stable-stringify": "^1.0.0",
    "husky": "^4.3.8",
    "jest": "^27.5.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.78.1",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "standard-version": "^9.5.0",
    "ts-jest": "^27.1.4",
    "tscpaths": "^0.0.9",
    "tslib": "^2.3.1",
    "typescript": "^4.8.2"
  },
  "dependencies": {
    "@blackglory/prelude": "^0.1.4",
    "extra-sort": "^0.1.1"
  }
}
